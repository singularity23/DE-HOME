{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-underline-offset",
          "-webkit-text-size-adjust",
          "orphans",
          "widows",
          "-webkit-tap-highlight-color",
          "@viewport",
          "@viewport"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off"
  }
}